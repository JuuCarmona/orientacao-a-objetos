class Cliente:
    def __init__(self,nome,contato,interesse,prazo):
        self.nome = nome
        self.contato = contato
        self.interesse = interesse
        self.prazo = prazo

cliente1 = Cliente(nome = 'phillips',contato=4198524445,interesse='comercial',prazo=30)
cliente2= Cliente(nome='apple',contato=18776253964,interesse='visual',prazo=58)
